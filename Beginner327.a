#include <iostream>
#include <algorithm>

using namespace std;


int main(){
    int n;
    string s;
    cin>>n>>s;
    bool flag=false;
    for(int i=0;i<n-1;i++){
        if((s[i]=='a' && s[i+1]=='b')||(s[i]=='b' && s[i+1]=='a')){
            cout<<"Yes"<<endl;
            flag=true;
            break;
        }
    }
    if(!flag){
        cout<<"No"<<endl;
    }
    return 0;
}
