#include <iostream>
#include <algorithm>
 
using namespace std;
typedef long long ll;
 
 
void solve(){
    int n;
    cin>>n;
    if(n%3==0){
        cout<<"Second"<<endl;
    }else{
        cout<<"First"<<endl;
    }
  //  cout<<(n%3==0?):"Second","First"<<"\n"<<;
}
 
int main() {
    ios::sync_with_stdio(false);  cin.tie(0);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
