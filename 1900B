#include <iostream>
#include <vector>
 
using namespace std;
 
void solve(){
    int a,b,c;
    cin>>a>>b>>c;
    int ans1, ans2, ans3;
    if(a%2==b%2 ){
        ans3=1;
    }else{
        ans3=0;
    }
    if(a%2==c%2 ){
        ans2=1;
    }else{
        ans2=0;
    }
    if(b%2==c%2 ){
        ans1=1;
    }else{
        ans1=0;
    }
    cout<<ans1<<" "<<ans2<<" "<<ans3<<endl;
}
 
int main(){
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
 
