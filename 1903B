#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
 
typedef long long ll;
#define yes cout<<"Yes\n"
#define no cout<<"No\n";
using namespace std;
 
 
void solve(){
    int n;
    cin>>n;
    vector<vector<int>> m(n+1, vector<int>(n+1));
    vector<int> arr(n+1);
    for(int i=1;i<=n;++i) arr[i]=(1<<30)-1;
    for(int i=1;i<=n;++i){
        for(int j=1;j<=n;++j){
            cin>>m[i][j];
            if(i!=j){
                arr[i] &= m[i][j];
                arr[j] &= m[i][j];
            }
        }
    }
    bool ok=true;
    for(int i=1;i<=n;++i){
        for(int j=1;j<=n;++j){
            if(i!=j && (arr[i] | arr[j]) != m[i][j]) ok=false;
        }
    }
    if(!ok){
        no;
    }else{
        yes;
        for(int i=1;i<=n;++i) cout<<arr[i]<<" ";
        cout<<"\n";
    }
}
 
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
