#include <iostream>
 
typedef long long ll;
const ll N=200000;
ll pref[N+5],ans[N+5];
int store[N+5],a[N+5],b[N+5],n;
 
using namespace std;
 
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        pref[0]=0;
        for(int i=0;i<=n;i++){
            ans[i]=0;
            store[i]=0;
        }
        for(int i=1;i<=n;i++){
            cin>>a[i];
        }
        for(int i=1;i<=n;i++){
            cin>>b[i];
            pref[i]=pref[i-1]+b[i]*1ll;
        }
        for(int i=1;i<=n;i++){
            int l=i, r=n;
            while(l<=r){
                int mid=(l+r)/2;
                if(pref[mid]-pref[i-1]>a[i]){
                    r=mid-1;
                }else{
                    l=mid+1;
                }
            }
            store[r]++;
            ans[r+1]+=a[i]-(pref[r]-pref[i-1]);
        }
        int last=0;
        for(int i=1;i<=n;i++){
            int current=last+1-store[i-1];
            last=current;
            ans[i]+=current*b[i]*1ll;
            cout<<ans[i]<<" ";
        }
        cout<<endl;
        
    }
    return 0;
}
