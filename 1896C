#include <cstring>
#include <iostream>
#include <queue>
#include <map>
#include <algorithm>
 
using namespace std;
const int N=200000;
int a[N+5];
 
void solve(){
    int n,x;
    cin>>n>>x;
    int a[N+5],b[N+5],c[N+5];
    map<int,queue<int>> store;
    int max=-1;
    for(int i=1;i<=n;i++){
        cin>>a[i];
        c[i]=a[i];
        if(a[i]>max) max=a[i];
    }
    for(int i=1;i<=n;i++){
        cin>>b[i];
    }
    sort(a+1,a+1+n);
    sort(b+1,b+1+n);
    for(int i=x;i>=1;i--){
        if(b[i]>=a[i+n-x]){
            cout<<"No\n";
            return;
        }else{
            store[a[i+n-x]].push(b[i]);
        }
    }
    for(int i=n-x;i>=1;i--){
        if(a[i]>b[i+x]){
            cout<<"No\n";
            return;
        }else{
            store[a[i]].push(b[i+x]);
        }
    }
    cout<<"YES\n";
    for(int i=1;i<=n;i++){
        int element=store[c[i]].front();
        cout<<element<<" ";
        store[c[i]].pop();
    }
    cout<<endl;
}
