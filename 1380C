#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
 
typedef long long ll;
#define yes cout<<"Yes\n"
#define no cout<<"No\n"
#define endl "\n"
using namespace std;
 
void solve(){
    ll n,x;
    cin>>n>>x;
    vector<ll> a(n+1);
    for(ll i=1;i<=n;++i){
        ll k;
        cin>>k;
        a[i]=(x-1)/k+1;
    }
    sort(a.begin()+1, a.end());
    ll prev=0,ans=0;
    for(ll i=1;i<=n;++i){
        if(a[i]<=i-prev){
            ans++;
            prev=i;
        }
    }
    cout<<ans<<endl;
}
 
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    ll t;
    cin>>t;
    while(t--){
        solve();
    }
 
}
