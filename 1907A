#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
 
typedef long long ll;
#define yes cout<<"Yes\n"
#define no cout<<"No\n"
#define endl "\n"
using namespace std;
 
ll gcd(ll a, ll b) {
    while (b != 0) {
        ll temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
 
void solve(){
    string pos;
    cin>>pos;
    char s1=pos[0], s2=pos[1];
    for(char i='1';i<='8';++i){
        if(s2!=i){
            cout<<s1<<i<<endl;
        }
    }
    for(char i='a';i<='h';++i){
        if(i!=s1){
            cout<<i<<s2<<endl;
        }
    }
}
 
int32_t main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    ll t;
    cin>>t;
    while(t--){
        solve();
    }
 
}
