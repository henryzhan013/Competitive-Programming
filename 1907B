#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <stack>
 
typedef long long ll;
#define yes cout<<"Yes\n"
#define no cout<<"No\n"
#define endl "\n"
using namespace std;
 
ll gcd(ll a, ll b) {
    while (b != 0) {
        ll temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
 
void solve(){
    string s;
    cin>>s;
    string ans;
    stack<char> s1,s2;
    ll i=s.length()-1;
    ll cnt1=0, cnt2=0;
    while(i>=0){
        if(s[i]=='B'){
            cnt1++;
        }else if(s[i]=='b'){
            cnt2++;
        }else{
            if(s[i]>='A' && s[i]<='Z'){
                if(cnt1>0){
                    cnt1--;
                }else{
                    ans+=s[i];
                }
            }else{
                if(cnt2>0){
                    cnt2--;
                }else{
                    ans+=s[i];
                }
            }
        }
        i--;
    }
    for(ll i=ans.length()-1;i>=0;i--){
        cout<<ans[i];
    }
    cout<<endl;
}
 
int32_t main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    ll t;
    cin>>t;
    while(t--){
        solve();
    }
 
}
