void solve(){
    ll n;
    cin>>n;
    vector<ll> a(n+1);
    for(int i=1;i<=n;i++){
        cin>>a[i];
    }
    vector<ll> suf(n+1);
    vector<bool> ad(n+1);
    suf[n]=a[n];
    if(suf[n]>=0){
        ad[n]=true;
    }
    for(int i=n-1;i>=1;i--){
        suf[i]=suf[i+1]+a[i];
        if(suf[i]>=0){
            ad[i]=true;
        }else{
            ad[i]=false;
        }
    }
    vector<ll> mul(n+1, 1);
    for(int i=2;i<=n;i++){
        if(ad[i]){
            mul[i]=mul[i-1]+1;
        }else{
            mul[i]=mul[i-1];
        }
    }
    ll ans=0;
    for(int i=1;i<=n;i++){
        ans+=mul[i]*a[i];
    }
 
    cout<<ans<<"\n";
}
 
int main(){
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
 
