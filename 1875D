#include <iostream>

using namespace std;
typedef long long ll;

const ll N=5000+5, Inf=0x3f3f3f3f3f3f3f3f;
ll n=0,m=0,a[N]={}, dp[N]={};

void init(){
    for(ll i=0;i<=n;i++) {
        a[i]=0;
        dp[i]=Inf;
    }
    n=m=0;
}

void solve(){
    cin>>n;
    for(ll i=1, x=0;i<=n;i++){
        cin>>x;
        if(x<n) a[x]++;
    }
    while(a[m]) m++;
    dp[m]=0;
    for(ll i=m;i>=1;i--){
        for(ll j=0;j<i;j++){
            dp[j]=min(dp[j],dp[i]+i*a[j]);
        }
    }
    cout<<dp[0]-m<<endl;
}

int main() {
    ios::sync_with_stdio(false);  cin.tie(0);
    memset(dp,0x3f, sizeof(dp));
    int t;
    cin>>t;
    while(t--){
        init();
        solve();
    }
    return 0;
}
