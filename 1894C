#include <iostream>
#include <vector>
#include <cstring>
using namespace std;
 
const int N=200000;
typedef long long ll;
 
void solve(){
    ll a[N+5];
    ll n,k;
    cin>>n>>k;
    bool skip=true;
    for(int i=1;i<=n;i++){
        cin>>a[i];
        if(a[i]>n) skip=false;
    }
    ll last=n;
    if(a[last]==n) skip=true;
    if(skip){
        cout<<"Yes"<<endl;
        return;
    }
    while(k--){
        if(a[last]>n){
            cout<<"No"<<endl;
            return;
        }
        ll store=last;
        if(last<=a[last])  {
            last+=n;
        }
        last-=a[store];
    }
    cout<<"Yes"<<endl;
}
 
 
int main(){
    int T;
    cin>>T;
    while(T--){
        solve();
    }
    return 0;
}
