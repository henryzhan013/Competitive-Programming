#include <iostream>
#include <map>
#include <set>
 
using namespace std;
typedef long long ll;
 
int main() {
    ios::sync_with_stdio(false);  cin.tie(0);
    set<ll> store={0};
    map<ll,ll> prev;
    int q;
    cin>>q;
    while(q--){
        char c;
        ll x;
        cin>>c>>x;
        if(c=='+'){
            store.insert(x);
        }else{
            ll base=x;
            auto it=prev.find(x);
            if(it != prev.end()){
                x = it -> second;
            }
            while(true){
                if(store.find(x)==store.end()){
                    cout<<x<<endl;
                    prev[base]=x;
                    break;
                }
                x+=base;
            }
        }
    }
    return 0;
}
