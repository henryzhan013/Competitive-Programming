#include <cstring>
#include <iostream>
#include <set>
#include <vector>
 
using namespace std;
 
 
void solve(){
    int n;
    cin>>n;
    string ans;
    int a[15];
    for(int i=1;i<=n;i++){
        cin>>a[i];
    }
    if(a[1]==1){
        cout<<"YES\n";
    }else{
        cout<<"no\n";
    }
}
 
int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
