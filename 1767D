#include <iostream>
 
using namespace std;

long long mypow(int times, long long base){
    long long ans=1;
    while(times--){
        ans*=base;
    }
    return ans;
}

int main(){
    int n,count0=0,count1=0;
    cin>>n;
    string s;
    cin>>s;
    for(int i=0;i<s.length();i++){
        if(s[i]=='0'){
            count0++;
        }else{
            count1++;
        }
    }
    long long low=mypow(count1, 2);
    long long high=mypow(n,2)-(mypow(count0, 2)-1);
    for(long long i=low;i<=high;i++){
        cout<<i<<" ";
    }
    cout<<endl;
    
    return 0;
}
