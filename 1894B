#include <iostream>
#include <vector>
#include <cstring>
using namespace std;
 
 
int main(){
    int T;
    cin>>T;
    while(T--){
        int n,cnt=0;
        cin>>n;
        int a[105],exist[105],ans[105];
        memset(a,0,sizeof(a));
        memset(exist,0,sizeof(exist));
        memset(ans,0,sizeof(ans));
        for(int i=1;i<=n;++i){
            cin>>a[i];
            if(exist[a[i]]==1){
                cnt++;
                exist[a[i]]++;
                if(cnt==1){
                    ans[i]=2;
                }else if(cnt==2){
                    ans[i]=3;
                }else{
                    ans[i]=1;
                }
            }else{
                ans[i]=1;
                exist[a[i]]++;
            }
        }
        if(cnt<2){
            cout<<-1;
        }else{
            for(int i=1;i<=n;i++){
                cout<<ans[i]<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}
